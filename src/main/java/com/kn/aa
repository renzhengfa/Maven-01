  `borrow_user_name` varchar(8) CHARACTER SET utf8 COLLATE utf8_bin NOT NULL COMMENT '借阅人姓名',
  `apply_user_id` int(11) NOT NULL COMMENT '申请人id',
  `apply_user_name` varchar(20) CHARACTER SET utf8 COLLATE utf8_bin NOT NULL COMMENT '申请人姓名',
  `fk_file_id` bigint(17) NOT NULL COMMENT '档案或者档案盒id',
  `fk_file_num` varchar(100) CHARACTER SET utf8 COLLATE utf8_bin DEFAULT NULL COMMENT '档号',
  `fk_file_name` text CHARACTER SET utf8 COLLATE utf8_bin NOT NULL COMMENT '题名',
  `fk_secret_id` int(11) NOT NULL COMMENT '密级id',
  `fk_secret_name` varchar(6) CHARACTER SET utf8 COLLATE utf8_bin NOT NULL COMMENT '密级名称',
  `bar_code` varchar(16) CHARACTER SET utf8 COLLATE utf8_bin DEFAULT NULL COMMENT '条形码',
  `rfid` varchar(24) CHARACTER SET utf8 COLLATE utf8_bin DEFAULT NULL COMMENT '电子标签',
  `fk_type_id` int(11) NOT NULL COMMENT '档案类别id',
  `fk_type_name` varchar(32) CHARACTER SET utf8 COLLATE utf8_bin NOT NULL COMMENT '档案类别名称',
  `is_box` tinyint(1) DEFAULT NULL COMMENT '是否为档案盒',
  `detail` json COMMENT '借阅详情',
  `process_id` varchar(20) CHARACTER SET utf8 COLLATE utf8_bin NOT NULL COMMENT '流程实例',
  `create_time` datetime(0) NOT NULL COMMENT '创建时间',
  `update_time` datetime(0) NOT NULL COMMENT '修改时间',
  `should_return_time` datetime(0) DEFAULT NULL COMMENT '应还时间',
  `real_return_time` datetime(0) DEFAULT NULL COMMENT '实际归还时间',
  `state` varchar(6) CHARACTER SET utf8 COLLATE utf8_bin NOT NULL COMMENT '当前状态\r\nlend(已借出)     back(已归还)     lose(丢失)\r\ntbt(待取)           cance(取消借阅)',
  `borrow_unit_id` int(11) DEFAULT NULL COMMENT '借阅单位id',
  `borrow_unit_name` varchar(20) CHARACTER SET utf8 COLLATE utf8_bin DEFAULT NULL COMMENT '借阅单位名称',
  `fk_auditor_id` int(11) DEFAULT NULL COMMENT '审核人id',
