-- ----------------------------
-- Table structure for equ_tb_equ_air_data_history_1
-- ----------------------------
DROP TABLE IF EXISTS `equ_tb_equ_air_data_history_1`;
CREATE TABLE `equ_tb_equ_air_data_history_1`  (
  `id` int(11) NOT NULL DEFAULT 0,
  `fk_store_id` int(5) NOT NULL COMMENT '所属库房编号',
  `fk_store_name` varchar(15) CHARACTER SET utf8 COLLATE utf8_general_ci NOT NULL COMMENT '库房名称',
  `fk_equ_id` int(11) NOT NULL COMMENT '采集器（监测点）id',
  `wd` double(6, 3) DEFAULT 0.000 COMMENT '温度',
