  `auditor_time` datetime(0) DEFAULT NULL COMMENT '审核时间',
  `task_id` varchar(20) CHARACTER SET utf8 COLLATE utf8_bin DEFAULT NULL COMMENT '流程运行id',
  `attachment` varchar(255) CHARACTER SET utf8 COLLATE utf8_bin DEFAULT NULL COMMENT '附件',
