			NEW.bar_code,
			NEW.fk_type_id,
			NEW.file_name,
			NEW.is_box,
			0,
			NEW.state,
			NEW.fk_location_id,
			NEW.file_num,
			NOW( )
		);
END
;;
delimiter ;

-- ----------------------------
-- Triggers structure for table arc_tb_file_2
-- ----------------------------
DROP TRIGGER IF EXISTS `arc_tb_file_2_updates`;
delimiter ;;
CREATE DEFINER = `root`@`localhost` TRIGGER `arc_tb_file_2_updates` AFTER UPDATE ON `arc_tb_file_2` FOR EACH ROW BEGIN
		UPDATE arc_tb_file_mapper
		SET rfid = NEW.rfid,
		bar_code = NEW.bar_code,
		fk_file_type_id = NEW.fk_type_id,
		fk_file_name = NEW.file_name,
		is_box = NEW.is_box,
		deleted = 0,
		state = NEW.state,
		fk_location_id = NEW.fk_location_id,
		file_num = NEW.file_num
	WHERE
	fk_file_id = NEW.id;
END
;;
delimiter ;

-- ----------------------------
-- Triggers structure for table arc_tb_file_2
-- ----------------------------
DROP TRIGGER IF EXISTS `arc_tb_file_2_deletes`;
delimiter ;;
CREATE DEFINER = `root`@`localhost` TRIGGER `arc_tb_file_2_deletes` AFTER DELETE ON `arc_tb_file_2` FOR EACH ROW BEGIN
		UPDATE arc_tb_file_mapper
		SET deleted = 1
	WHERE
	fk_file_id = OLD.id;
END
;;
delimiter ;

-- ----------------------------
-- Triggers structure for table arc_tb_file_3
-- ----------------------------
DROP TRIGGER IF EXISTS `arc_tb_file_3_inserts`;
delimiter ;;
CREATE DEFINER = `root`@`localhost` TRIGGER `arc_tb_file_3_inserts` AFTER INSERT ON `arc_tb_file_3` FOR EACH ROW BEGIN
		INSERT INTO `arc_tb_file_mapper` ( `fk_file_id`, `rfid`, `bar_code`, `fk_file_type_id`, `fk_file_name`, `is_box`, `deleted`, `state`, `fk_location_id`, `file_num`, `create_time` )
	VALUES
		(
			NEW.id,
			NEW.rfid,
			NEW.bar_code,
			NEW.fk_type_id,
			NEW.file_name,
			NEW.is_box,
			0,
			NEW.state,
			NEW.fk_location_id,
			NEW.file_num,
			NOW( )
		);
END
;;
delimiter ;

-- ----------------------------
-- Triggers structure for table arc_tb_file_3
-- ----------------------------
DROP TRIGGER IF EXISTS `arc_tb_file_3_updates`;
delimiter ;;
CREATE DEFINER = `root`@`localhost` TRIGGER `arc_tb_file_3_updates` AFTER UPDATE ON `arc_tb_file_3` FOR EACH ROW BEGIN
		UPDATE arc_tb_file_mapper
		SET rfid = NEW.rfid,
		bar_code = NEW.bar_code,
		fk_file_type_id = NEW.fk_type_id,
		fk_file_name = NEW.file_name,
		is_box = NEW.is_box,
		deleted = 0,
		state = NEW.state,
		fk_location_id = NEW.fk_location_id,
		file_num = NEW.file_num
	WHERE
	fk_file_id = NEW.id;
END
;;
delimiter ;

-- ----------------------------
-- Triggers structure for table arc_tb_file_3
-- ----------------------------
DROP TRIGGER IF EXISTS `arc_tb_file_3_deletes`;
delimiter ;;
CREATE DEFINER = `root`@`localhost` TRIGGER `arc_tb_file_3_deletes` AFTER DELETE ON `arc_tb_file_3` FOR EACH ROW BEGIN
		UPDATE arc_tb_file_mapper
		SET deleted = 1
	WHERE
	fk_file_id = OLD.id;
END
;;
delimiter ;

-- ----------------------------
-- Triggers structure for table arc_tb_location
-- ----------------------------
DROP TRIGGER IF EXISTS `locationUpdate`;
delimiter ;;
CREATE DEFINER = `root`@`localhost` TRIGGER `locationUpdate` BEFORE UPDATE ON `arc_tb_location` FOR EACH ROW BEGIN
 SET NEW.location_name=
 concat(NEW.fk_store_name,'',NEW.fk_region_name,NEW.col_num,'列',NEW.div_num,'节',NEW.lays_num,'层',NEW.direction,'方第',NEW.number,'号');
end
;;
delimiter ;

-- ----------------------------
-- Triggers structure for table sto_tb_region
-- ----------------------------
DROP TRIGGER IF EXISTS `regionUpdate`;
delimiter ;;
CREATE DEFINER = `root`@`localhost` TRIGGER `regionUpdate` AFTER UPDATE ON `sto_tb_region` FOR EACH ROW BEGIN
UPDATE arc_tb_location SET fk_store_id=NEW.fk_store_id,fk_store_name=NEW.fk_store_name,fk_region_id=NEW.id,fk_region_name=NEW.region_name,fk_region_num=NEW.region_num WHERE fk_region_id = NEW.id;
end
;;
delimiter ;

-- ----------------------------
-- Triggers structure for table sto_tb_store
-- ----------------------------
DROP TRIGGER IF EXISTS `storeUpdate`;
delimiter ;;
CREATE DEFINER = `root`@`localhost` TRIGGER `storeUpdate` BEFORE UPDATE ON `sto_tb_store` FOR EACH ROW BEGIN
UPDATE sto_tb_region SET fk_store_id=NEW.id,fk_store_name=NEW.store_name WHERE fk_store_id = NEW.id;
end
;;
delimiter ;

SET FOREIGN_KEY_CHECKS = 1;


CREATE TABLE `sto_tb_region` (
  `id` int(11) NOT NULL AUTO_INCREMENT COMMENT 'id',
  `region_num` int(2) NOT NULL COMMENT '区号',
  `width` int(4) DEFAULT NULL COMMENT '宽度',
  `reqest_ip` varchar(16) COLLATE utf8_bin DEFAULT NULL COMMENT '请求地址',
  `http_port` int(5) DEFAULT NULL COMMENT 'http端口号',
  `tcp_port` int(5) DEFAULT NULL COMMENT 'tcp端口号',
  `region_name` varchar(4) COLLATE utf8_bin DEFAULT NULL COMMENT '区名称',
  `gdl_type` varchar(10) COLLATE utf8_bin NOT NULL COMMENT 'left,right',
  `fk_store_id` int(11) NOT NULL COMMENT '库房id',
  `fk_store_name` varchar(12) COLLATE utf8_bin NOT NULL COMMENT '库房名称',
  `cols` int(2) DEFAULT NULL COMMENT '列数',
  `lays` int(2) DEFAULT NULL COMMENT '层数',
  `divs` int(2) DEFAULT NULL COMMENT '节数',
  `bind_id` int(11) DEFAULT NULL COMMENT '固定列为中间的时候绑定灵一个区',
  `speed` int(10) DEFAULT NULL COMMENT '速度',
  `deleted` int(2) NOT NULL COMMENT '是否删除0未删除1删除',
  `fk_equ_id` int(8) DEFAULT NULL COMMENT '关联设备id',
  `layer_capacity` int(8) DEFAULT NULL COMMENT '层容量',
  `update_time` datetime DEFAULT NULL ON UPDATE CURRENT_TIMESTAMP COMMENT '更新时间',
  PRIMARY KEY (`id`,`region_num`) USING BTREE
) ENGINE=InnoDB AUTO_INCREMENT=80 DEFAULT CHARSET=utf8 COLLATE=utf8_bin ROW_FORMAT=DYNAMIC COMMENT='区表\r\n';

CREATE DEFINER=`root`@`localhost` TRIGGER `regionUpdate` AFTER UPDATE ON `sto_tb_region` FOR EACH ROW BEGIN
UPDATE arc_tb_location SET fk_store_id=NEW.fk_store_id,fk_store_name=NEW.fk_store_name,fk_region_id=NEW.id,fk_region_name=NEW.region_name,fk_region_num=NEW.region_num WHERE fk_region_id = NEW.id;
end;