  `transfer_user_id` int(11) DEFAULT NULL COMMENT '移交人id',
  `transfer_user_name` varchar(10) CHARACTER SET utf8 COLLATE utf8_bin DEFAULT NULL COMMENT '移交人名称',
  `create_time` datetime(0) DEFAULT NULL COMMENT '创建时间',
  `update_time` datetime(0) DEFAULT NULL COMMENT '移交时间',
  `fk_transfer_unit_id` int(11) DEFAULT NULL COMMENT '移交单位id',
  `fk_transfer_unit_name` varchar(20) CHARACTER SET utf8 COLLATE utf8_bin DEFAULT NULL COMMENT '移交单位名称',
  `fk_receive_unit_id` int(11) DEFAULT NULL COMMENT '接收单位id',
  `fk_receive_unit_name` varchar(20) CHARACTER SET utf8 COLLATE utf8_bin DEFAULT NULL COMMENT '接收单位',
  `apply_user_id` int(11) DEFAULT NULL COMMENT '申请人id',
  `apply_user_name` varchar(10) CHARACTER SET utf8 COLLATE utf8_bin DEFAULT NULL COMMENT '申请名称',
  `detail` json COMMENT '详情',
  PRIMARY KEY (`id`) USING BTREE
) ENGINE = InnoDB AUTO_INCREMENT = 5 CHARACTER SET = utf8 COLLATE = utf8_bin COMMENT = '移交批次' ROW_FORMAT = Dynamic;

