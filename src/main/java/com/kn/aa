  UNIQUE INDEX `phone`(`phone`) USING BTREE,
  UNIQUE INDEX `job_number`(`job_number`) USING BTREE,
  INDEX `id_card`(`id_card`) USING BTREE
) ENGINE = InnoDB AUTO_INCREMENT = 81 CHARACTER SET = utf8 COLLATE = utf8_general_ci COMMENT = '用户表' ROW_FORMAT = Dynamic;

