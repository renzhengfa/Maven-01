-- ----------------------------
-- Table structure for equ_tb_equ_alarm
-- ----------------------------
DROP TABLE IF EXISTS `equ_tb_equ_alarm`;
CREATE TABLE `equ_tb_equ_alarm`  (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `create_time` timestamp(0) NOT NULL DEFAULT CURRENT_TIMESTAMP COMMENT '创建时间',
  `fk_store_id` int(11) NOT NULL COMMENT '库房编号',
  `fk_store_name` varchar(255) CHARACTER SET utf8 COLLATE utf8_general_ci NOT NULL COMMENT '库房名称',
  `fk_equ_id` int(11) NOT NULL COMMENT '设备监测点id',
  `fk_equ_num` int(11) NOT NULL DEFAULT 0 COMMENT '设备编号',
  `fk_equ_name` varchar(255) CHARACTER SET utf8 COLLATE utf8_general_ci NOT NULL COMMENT '设备监测点名称',
  `alarm_key` varchar(255) CHARACTER SET utf8 COLLATE utf8_general_ci NOT NULL COMMENT '报警关键字',
  `alarm_msg` varchar(255) CHARACTER SET utf8 COLLATE utf8_general_ci NOT NULL COMMENT '报警信息',
  `alarm_value` double DEFAULT NULL COMMENT '报警值',
  `alarm_unit` varchar(255) CHARACTER SET utf8 COLLATE utf8_general_ci DEFAULT '' COMMENT '单位',
  `state` varchar(10) CHARACTER SET utf8 COLLATE utf8_general_ci DEFAULT NULL COMMENT '处理状态：未处理:untreat,已处理:process,忽略:ignore',
  `level` tinyint(1) DEFAULT NULL COMMENT '报警级别:一级、二级、三级、四级',
  `fk_alarm_type_id` int(11) DEFAULT NULL COMMENT '报警类别id',
  `fk_alarm_type_name` varchar(255) CHARACTER SET utf8 COLLATE utf8_general_ci DEFAULT NULL COMMENT '报警类别名称',
  PRIMARY KEY (`id`) USING BTREE
) ENGINE = InnoDB AUTO_INCREMENT = 1 CHARACTER SET = utf8 COLLATE = utf8_general_ci COMMENT = '报警信息表' ROW_FORMAT = Dynamic;

