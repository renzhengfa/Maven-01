  `BYTEARRAY_ID_` varchar(64) CHARACTER SET utf8 COLLATE utf8_bin DEFAULT NULL COMMENT '字节表的ID\r\n\r\n（ACT_GE_BYTEARRAY）',
  `DOUBLE_` double DEFAULT NULL COMMENT '存储变量类型为Double',
