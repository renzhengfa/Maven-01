  `ACTION_` varchar(255) CHARACTER SET utf8 COLLATE utf8_bin DEFAULT NULL COMMENT '	\r\n行为类型',
  `MESSAGE_` varchar(4000) CHARACTER SET utf8 COLLATE utf8_bin DEFAULT NULL COMMENT '基本内容(	\r\n用于存放流程产生的信息，比如审批意见)',
  `FULL_MSG_` longblob COMMENT '	\r\n全部内容(附件地址)',
  PRIMARY KEY (`ID_`) USING BTREE
) ENGINE = InnoDB CHARACTER SET = utf8 COLLATE = utf8_bin COMMENT = '历史意见表' ROW_FORMAT = Dynamic;

