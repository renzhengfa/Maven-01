-- ----------------------------
-- Function structure for diguihanshu
-- ----------------------------
DROP FUNCTION IF EXISTS `diguihanshu`;
delimiter ;;
CREATE DEFINER=`root`@`localhost` FUNCTION `diguihanshu`(areaId INT, roleId VARCHAR(50)) RETURNS varchar(4000) CHARSET utf8 COLLATE utf8_bin
BEGIN
DECLARE sid VARCHAR(4000);
DECLARE sidChd VARCHAR(4000);

SET sid='$';
SET sidChd = CAST(areaId AS CHAR);

WHILE sidChd IS NOT NULL DO
SET sid= CONCAT(sid,',',sidChd);
SELECT GROUP_CONCAT(m.id) INTO sidChd FROM auth_tb_menu m
LEFT JOIN auth_fk_role_menu rm ON m.id = rm.fk_menu_id
 WHERE
m.disabled=0
and FIND_IN_SET(rm.fk_role_id,roleId)
and FIND_IN_SET(m.fk_parent_id,sidChd)>0;
END WHILE;
RETURN sid;
END
;;
delimiter ;

-- ----------------------------
-- Triggers structure for table arc_tb_file
-- ----------------------------
DROP TRIGGER IF EXISTS `fileInsert`;
delimiter ;;
CREATE DEFINER = `root`@`localhost` TRIGGER `fileInsert` BEFORE INSERT ON `arc_tb_file` FOR EACH ROW BEGIN
INSERT INTO `arc_tb_file_mapper`(`fk_file_id`, `rfid`, `bar_code`, `fk_file_type_id`, `fk_file_name`, `is_box`, `deleted`, `state`, `fk_location_id`, `file_num`, `create_time`) VALUES (NEW.id, NEW.rfid, NEW.bar_code, NEW.fk_type_id, NEW.file_name, NEW.is_box, 0, NEW.state, NEW.fk_location_id, NEW.file_num, NOW());
end
;;
delimiter ;

-- ----------------------------
-- Triggers structure for table arc_tb_file
-- ----------------------------
DROP TRIGGER IF EXISTS `fileUpdate`;
delimiter ;;
CREATE DEFINER = `root`@`localhost` TRIGGER `fileUpdate` BEFORE UPDATE ON `arc_tb_file` FOR EACH ROW BEGIN
UPDATE`arc_tb_file_mapper` SET `rfid` = NEW.rfid, `bar_code` = NEW.bar_code, `fk_file_type_id` = NEW.fk_type_id, `fk_file_name` = NEW.file_name, `is_box` = NEW.is_box, `deleted` = 0, `state` = NEW.state, `fk_location_id` = NEW.fk_location_id, `file_num` =  NEW.file_num WHERE fk_file_id = NEW.id;
end
;;
delimiter ;

-- ----------------------------
-- Triggers structure for table arc_tb_file
-- ----------------------------
DROP TRIGGER IF EXISTS `fileDelete`;
delimiter ;;
CREATE DEFINER = `root`@`localhost` TRIGGER `fileDelete` BEFORE DELETE ON `arc_tb_file` FOR EACH ROW BEGIN
	delete FROM arc_tb_file_mapper where fk_file_id=OLD.id;
end
;;
delimiter ;

-- ----------------------------
-- Triggers structure for table arc_tb_file_1
-- ----------------------------
DROP TRIGGER IF EXISTS `arc_tb_file_1_inserts`;
delimiter ;;
CREATE DEFINER = `root`@`localhost` TRIGGER `arc_tb_file_1_inserts` AFTER INSERT ON `arc_tb_file_1` FOR EACH ROW BEGIN
		INSERT INTO `arc_tb_file_mapper` ( `fk_file_id`, `rfid`, `bar_code`, `fk_file_type_id`, `fk_file_name`, `is_box`, `deleted`, `state`, `fk_location_id`, `file_num`, `create_time` )
	VALUES
		(
			NEW.id,
			NEW.rfid,
			NEW.bar_code,
			NEW.fk_type_id,
			NEW.file_name,
			NEW.is_box,
			0,
			NEW.state,
			NEW.fk_location_id,
			NEW.file_num,
			NOW( )
		);
END
;;
delimiter ;

-- ----------------------------
-- Triggers structure for table arc_tb_file_1
-- ----------------------------
DROP TRIGGER IF EXISTS `arc_tb_file_1_updates`;
delimiter ;;
CREATE DEFINER = `root`@`localhost` TRIGGER `arc_tb_file_1_updates` AFTER UPDATE ON `arc_tb_file_1` FOR EACH ROW BEGIN
		UPDATE arc_tb_file_mapper
		SET rfid = NEW.rfid,
		bar_code = NEW.bar_code,
		fk_file_type_id = NEW.fk_type_id,
		fk_file_name = NEW.file_name,
		is_box = NEW.is_box,
		deleted = 0,
		state = NEW.state,
		fk_location_id = NEW.fk_location_id,
		file_num = NEW.file_num
	WHERE
	fk_file_id = NEW.id;
END
;;
delimiter ;

-- ----------------------------
-- Triggers structure for table arc_tb_file_1
-- ----------------------------
DROP TRIGGER IF EXISTS `arc_tb_file_1_deletes`;
delimiter ;;
CREATE DEFINER = `root`@`localhost` TRIGGER `arc_tb_file_1_deletes` AFTER DELETE ON `arc_tb_file_1` FOR EACH ROW BEGIN
		UPDATE arc_tb_file_mapper
		SET deleted = 1
	WHERE
	fk_file_id = OLD.id;
END
;;
delimiter ;

