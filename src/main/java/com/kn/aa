  `fk_file_type_id` int(11) NOT NULL COMMENT '分表的表名',
  PRIMARY KEY (`id`) USING BTREE
) ENGINE = InnoDB AUTO_INCREMENT = 1 CHARACTER SET = utf8 COLLATE = utf8_bin ROW_FORMAT = Dynamic;

-- ----------------------------
-- Table structure for arc_tb_arc_type
-- ----------------------------
DROP TABLE IF EXISTS `arc_tb_arc_type`;
