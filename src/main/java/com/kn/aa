-- ----------------------------
-- Table structure for act_re_procdef
-- ----------------------------
DROP TABLE IF EXISTS `act_re_procdef`;
CREATE TABLE `act_re_procdef`  (
  `ID_` varchar(64) CHARACTER SET utf8 COLLATE utf8_bin NOT NULL,
  `REV_` int(11) DEFAULT NULL COMMENT '数据版本号',
  `CATEGORY_` varchar(255) CHARACTER SET utf8 COLLATE utf8_bin DEFAULT NULL COMMENT '流程定义分类',
  `NAME_` varchar(255) CHARACTER SET utf8 COLLATE utf8_bin DEFAULT NULL COMMENT '流程定义名称',
  `KEY_` varchar(255) CHARACTER SET utf8 COLLATE utf8_bin NOT NULL COMMENT '流程定义的key',
  `VERSION_` int(11) NOT NULL COMMENT '	\r\n版本',
  `DEPLOYMENT_ID_` varchar(64) CHARACTER SET utf8 COLLATE utf8_bin DEFAULT NULL COMMENT '流程定义对应部署数据ID',
  `RESOURCE_NAME_` varchar(4000) CHARACTER SET utf8 COLLATE utf8_bin DEFAULT NULL COMMENT '流程定义对应的资源名称',
  `DGRM_RESOURCE_NAME_` varchar(4000) CHARACTER SET utf8 COLLATE utf8_bin DEFAULT NULL COMMENT '流程对应的流程图资源名称',
  `DESCRIPTION_` varchar(4000) CHARACTER SET utf8 COLLATE utf8_bin DEFAULT NULL COMMENT '	\r\n描述',
  `HAS_START_FORM_KEY_` tinyint(4) DEFAULT NULL COMMENT '	\r\nstart节点是否存在formKey\r\n\r\n 0否  1是',
  `HAS_GRAPHICAL_NOTATION_` tinyint(4) DEFAULT NULL COMMENT '1 激活 2挂起',
  `SUSPENSION_STATE_` int(11) DEFAULT NULL COMMENT '流程定义是否是中止状态（激活：1 中止：2）',
  `TENANT_ID_` varchar(255) CHARACTER SET utf8 COLLATE utf8_bin DEFAULT '',
  PRIMARY KEY (`ID_`) USING BTREE,
  UNIQUE INDEX `ACT_UNIQ_PROCDEF`(`KEY_`, `VERSION_`, `TENANT_ID_`) USING BTREE
) ENGINE = InnoDB CHARACTER SET = utf8 COLLATE = utf8_bin COMMENT = '流程定义表' ROW_FORMAT = Dynamic;

