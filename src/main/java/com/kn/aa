  `id` int(11) NOT NULL AUTO_INCREMENT COMMENT '编号',
  `type_name` varchar(16) CHARACTER SET utf8 COLLATE utf8_bin NOT NULL COMMENT '类型名称',
  `type_info` varchar(128) CHARACTER SET utf8 COLLATE utf8_bin DEFAULT NULL COMMENT '类型简介',
  `fk_parent_id` int(11) NOT NULL COMMENT '上级编号',
  `word_tpl_address` varchar(128) CHARACTER SET utf8 COLLATE utf8_bin NOT NULL COMMENT 'word模板地址',
  `fk_borrow_flow` int(11) NOT NULL COMMENT '借阅流程',
  `fk_destroy_flow` int(11) NOT NULL COMMENT '销毁流程',
  `fk_transfer_flow` int(11) NOT NULL COMMENT '移交流程',
  `fk_regress_flow` int(11) NOT NULL COMMENT '归档流程',
  `create_time` datetime(0) DEFAULT NULL COMMENT '创建时间',
  `update_time` datetime(0) DEFAULT NULL COMMENT '更新时间',
  `html_tpl` mediumtext CHARACTER SET utf8 COLLATE utf8_bin COMMENT 'html文档渲染',
  `arc_tpl` json COMMENT '模板',
  `deleted` tinyint(1) DEFAULT 0 COMMENT '删除标志',
  PRIMARY KEY (`id`) USING BTREE
) ENGINE = InnoDB AUTO_INCREMENT = 4 CHARACTER SET = utf8 COLLATE = utf8_bin COMMENT = '档案类别' ROW_FORMAT = Dynamic;

-- ----------------------------
-- Table structure for arc_tb_attachment
-- ----------------------------
DROP TABLE IF EXISTS `arc_tb_attachment`;
CREATE TABLE `arc_tb_attachment`  (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `file_old_name` varchar(255) CHARACTER SET utf8 COLLATE utf8_bin DEFAULT NULL COMMENT '文件原名称',
  `file_full_name` varchar(128) CHARACTER SET utf8 COLLATE utf8_bin DEFAULT NULL COMMENT '文件上传后的完整名称',
  `file_suffix` varchar(20) CHARACTER SET utf8 COLLATE utf8_bin DEFAULT NULL COMMENT '文件后缀',
  `file_prefix` varchar(20) CHARACTER SET utf8 COLLATE utf8_bin DEFAULT NULL COMMENT '文件前缀',
  `file_storage_path` varchar(255) CHARACTER SET utf8 COLLATE utf8_bin DEFAULT NULL COMMENT '文件存储路径',
  `file_size` double(10, 2) DEFAULT NULL COMMENT '文件大小,单位kb',
  `create_time` datetime(0) DEFAULT NULL COMMENT '创建时间',
  `deleted` int(1) DEFAULT 0 COMMENT '是否删除',
  PRIMARY KEY (`id`) USING BTREE
) ENGINE = InnoDB AUTO_INCREMENT = 1 CHARACTER SET = utf8 COLLATE = utf8_bin COMMENT = '文件附件表' ROW_FORMAT = Dynamic;

